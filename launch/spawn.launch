<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <param name="robot_description" command="$(find xacro)/xacro '$(find mtracker)/urdf/m2wr.xacro'"/>    
 
    <arg name="x" default="0"/>
    <arg name="y" default="0"/>
    <arg name="z" default="0.5"/>

    <node name="mybot_spawn" pkg="gazebo_ros" type="spawn_model" output="screen" args="-urdf -param robot_description -model mtracker -x $(arg x) -y $(arg y) -z $(arg z)"/>

    <rosparam file="$(find mtracker)/config/diff_drive_controller.yaml" command="load" />
    <rosparam file="$(find mtracker)/config/gazebo_ros_control_params.yaml" command="load" />

    <node name="mobile_base_controller_manager" pkg="controller_manager" type="spawner" respawn="false" output="screen" args="mobile_base_controller" />

</launch>
